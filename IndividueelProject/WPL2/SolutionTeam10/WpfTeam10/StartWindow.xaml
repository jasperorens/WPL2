<Window x:Class="WpfTeam10.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTeam10"
        mc:Ignorable="d"
        Title="StartWindow" Height="450" Width="800" Loaded="StartWindow_Loaded">
    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top" Height="25">
                  <MenuItem Height="25" Header="File">
                  <MenuItem Height="25" Header="Student" Click="StudentWindow_Click"></MenuItem>
                  <MenuItem Height="25" Header="SqlWindow" Click="SqlWindow_Click"/>
                  <MenuItem Height="25" Header="Close" Click="Close_Click"></MenuItem>
                </MenuItem>
            </Menu>
            <StatusBar DockPanel.Dock="Bottom" Height="25" ></StatusBar>
            <ListBox Width="250" Height="50" Name="LbStartWindow"></ListBox>
        </DockPanel>
    </Grid>
</Window>
